<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Company.
    -->
    <changeSet id="20191103190759-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="employee_id"
                                 baseTableName="company"
                                 constraintName="fk_company_employee_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"/>

        <addForeignKeyConstraint baseColumnNames="industry_tipe_id"
                                 baseTableName="company"
                                 constraintName="fk_company_industry_tipe_id"
                                 referencedColumnNames="id"
                                 referencedTableName="industry_type"/>

    </changeSet>
</databaseChangeLog>
